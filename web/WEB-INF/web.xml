<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>wbq</servlet-name>
        <servlet-class>com.wbq.servlet.test.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>wbq</servlet-name>
        <url-pattern>/myservlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>lifeServlet</servlet-name>
        <servlet-class>com.wbq.servlet.test.LifeServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>lifeServlet</servlet-name>
        <url-pattern>/lifes</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>genericServlet</servlet-name>
        <servlet-class>com.wbq.servlet.test.GenricServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>genericServlet</servlet-name>
        <url-pattern>/gn</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>httpServlet</servlet-name>
        <servlet-class>com.wbq.servlet.test.HttpServlets</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>httpServlet</servlet-name>
        <url-pattern>/hs</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>myfilter</filter-name>
        <filter-class>com.wbq.servlet.test.filter.MyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>myfilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>